<svg width="120" height="120" viewBox="0 0 120 120" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FFC91A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFB300;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Outer circle with circuit pattern -->
  <circle cx="60" cy="60" r="58" fill="url(#goldGradient)" stroke="#FFD700" stroke-width="2"/>
  
  <!-- Circuit board pattern in the border -->
  <g opacity="0.3">
    <!-- Horizontal circuit lines -->
    <line x1="20" y1="25" x2="35" y2="25" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="85" y1="25" x2="100" y2="25" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="20" y1="95" x2="35" y2="95" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="85" y1="95" x2="100" y2="95" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    
    <!-- Vertical circuit lines -->
    <line x1="25" y1="20" x2="25" y2="35" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="95" y1="20" x2="95" y2="35" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="25" y1="85" x2="25" y2="100" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    <line x1="95" y1="85" x2="95" y2="100" stroke="#0a1929" stroke-width="1.5" stroke-linecap="round"/>
    
    <!-- Circuit nodes -->
    <circle cx="30" cy="25" r="2.5" fill="#0a1929"/>
    <circle cx="90" cy="25" r="2.5" fill="#0a1929"/>
    <circle cx="25" cy="30" r="2.5" fill="#0a1929"/>
    <circle cx="95" cy="30" r="2.5" fill="#0a1929"/>
    <circle cx="30" cy="95" r="2.5" fill="#0a1929"/>
    <circle cx="90" cy="95" r="2.5" fill="#0a1929"/>
    <circle cx="25" cy="90" r="2.5" fill="#0a1929"/>
    <circle cx="95" cy="90" r="2.5" fill="#0a1929"/>
    
    <!-- Diagonal connections -->
    <line x1="30" y1="25" x2="25" y2="30" stroke="#0a1929" stroke-width="1" opacity="0.5"/>
    <line x1="90" y1="25" x2="95" y2="30" stroke="#0a1929" stroke-width="1" opacity="0.5"/>
    <line x1="30" y1="95" x2="25" y2="90" stroke="#0a1929" stroke-width="1" opacity="0.5"/>
    <line x1="90" y1="95" x2="95" y2="90" stroke="#0a1929" stroke-width="1" opacity="0.5"/>
  </g>
  
  <!-- Handshake icon in center -->
  <g transform="translate(60, 60)">
    <!-- Left hand -->
    <g transform="translate(-15, -8)">
      <!-- Wrist -->
      <rect x="0" y="20" width="8" height="12" rx="2" fill="url(#goldGradient)" stroke="#FFB300" stroke-width="0.5"/>
      <!-- Hand -->
      <ellipse cx="4" cy="12" rx="6" ry="10" fill="url(#goldGradient)" stroke="#FFB300" stroke-width="0.5"/>
      <!-- Fingers -->
      <ellipse cx="1" cy="8" rx="1.5" ry="4" fill="url(#goldGradient)"/>
      <ellipse cx="4" cy="6" rx="1.5" ry="5" fill="url(#goldGradient)"/>
      <ellipse cx="7" cy="8" rx="1.5" ry="4" fill="url(#goldGradient)"/>
    </g>
    
    <!-- Right hand -->
    <g transform="translate(15, -8) scale(-1, 1)">
      <!-- Wrist -->
      <rect x="0" y="20" width="8" height="12" rx="2" fill="url(#goldGradient)" stroke="#FFB300" stroke-width="0.5"/>
      <!-- Hand -->
      <ellipse cx="4" cy="12" rx="6" ry="10" fill="url(#goldGradient)" stroke="#FFB300" stroke-width="0.5"/>
      <!-- Fingers -->
      <ellipse cx="1" cy="8" rx="1.5" ry="4" fill="url(#goldGradient)"/>
      <ellipse cx="4" cy="6" rx="1.5" ry="5" fill="url(#goldGradient)"/>
      <ellipse cx="7" cy="8" rx="1.5" ry="4" fill="url(#goldGradient)"/>
    </g>
    
    <!-- Connection point (where hands meet) -->
    <circle cx="0" cy="0" r="3" fill="#FFD700" opacity="0.8" filter="url(#glow)"/>
  </g>
</svg>
